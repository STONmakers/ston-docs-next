m_eMethod = eCACHE_CLUSTER_METHOD_CACHE;
	const QCHAR* GetMethodStr();
	ECACHE_CLUSTER_METHOD m_eMethod;

	if( a_pElmtCluster->attribute(s_szCACHECONF_ATTR_METHOD) )
	{
		strValue = a_pElmtCluster->attribute(s_szCACHECONF_ATTR_METHOD).as_string();
		qString_Trim(strValue);
		qString_ToLower(strValue);

		if(strValue == s_szCACHECONF_VALUE_REDIRECT)
			m_eMethod = eCACHE_CLUSTER_METHOD_REDIRECT;
		else if(strValue == s_szCACHECONF_VALUE_BYPASS)
			m_eMethod = eCACHE_CLUSTER_METHOD_BYPASS;
		else
			m_eMethod = eCACHE_CLUSTER_METHOD_CACHE;
	}
	
	
	
	const QCHAR* CacheCluster::GetMethodStr()
{
	switch(m_eMethod)
	{
	case eCACHE_CLUSTER_METHOD_REDIRECT:	return s_szCACHECONF_VALUE_REDIRECT; break;
	case eCACHE_CLUSTER_METHOD_BYPASS:		return s_szCACHECONF_VALUE_BYPASS; break;
	case eCACHE_CLUSTER_METHOD_CACHE:		return s_szCACHECONF_VALUE_CACHE; break;
	}

	return "";
}